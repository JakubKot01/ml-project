{
    "subrandom": false,
    "data-storage": "disc",
    "random-seed": 0,
    "dataset-path": "./wikiart",
    "dataset-labels-path": "./wikiart-labels",
    "loader": {
        "batch-size": "128 MiB"
    },
    "global-palette": {
        "size": 1000,
        "patch-size": 1,
        "coverage": 0.01,
        "random": true,
        "batching-k-means": {
            "batch-size": 100,
            "max-iterations": 20,
            "number-of-clusters": 1000
        }
    },
    "local-palette": {
        "size": 128,
        "patch-size": 1,
        "k-neigh": 3,
        "coverage": 0.01,
        "random": true,
        "batching-k-means": {
            "batch-size": 100,
            "max-iterations": 20,
            "number-of-clusters": 128
        }
    },
    "hdf5": {
        "base-directory": "hdf5",
        "dataset": "256"
    }
}